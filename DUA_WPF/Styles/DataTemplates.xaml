<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:vm="clr-namespace:DUA_WPF.ViewModels"
                    xmlns:v="clr-namespace:DUA_WPF.Views"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    

    <DataTemplate DataType="{x:Type vm:DrawerListViewModel}">
        <v:DrawerList />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GroupsViewModel}">
        <v:GroupsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TemplatesViewModel}">
        <v:TemplatesView />
    </DataTemplate>



    <DataTemplate DataType="{x:Type vm:GroupViewModel}">
        <v:GroupView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TemplateViewModel}">
        <v:TemplateView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AlignmentTemplateViewModel}">
        <v:AlignmentTemplateView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CorridorStyleTemplateViewModel}">
        <v:CorridorStyleTemplateView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfileTemplateViewModel}">
        <v:ProfileTemplateView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProfileViewStyleViewModel}">
        <v:ProfileViewTemplateView />
    </DataTemplate>
</ResourceDictionary>